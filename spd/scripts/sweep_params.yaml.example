  seed:
    values: [0, 1]
  loss_metric_configs:
    - classname: "ImportanceMinimalityLoss"
      coeff:
        values: [0.1, 0.2]

# Experiment-specific parameters (override global)
tms_5-2:
  seed:
    values: [100, 200]  # Overrides global seed
  task_config:
    feature_probability:
      values: [0.05, 0.1]