+        font-weight: 500;
+    }
+
+    .mask-l0 {
+        color: #007bff;
+    }
+
+    .apply-btn {
+        width: 100%;
+        padding: 0.5rem;
+        background: #007bff;
+        color: white;
+        border: none;
+        border-radius: 4px;
+        cursor: pointer;
+        font-weight: 500;
+        transition: background 0.2s;
+    }
+
+    .apply-btn:hover {
+        background: #0056b3;
+    }
+
+    /* Scrollbar styling */
+    .masks-container::-webkit-scrollbar {
+        height: 6px;
+    }
+
+    .masks-container::-webkit-scrollbar-track {
+        background: #f1f1f1;
+        border-radius: 3px;
+    }
+
+    .masks-container::-webkit-scrollbar-thumb {
+        background: #888;
+        border-radius: 3px;
+    }
